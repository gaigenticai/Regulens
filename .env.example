# Regulens Agentic AI Compliance System - Environment Configuration
# Production-grade configuration template for cloud and on-premise deployment

# =============================================================================
# SYSTEM CONFIGURATION
# =============================================================================

# Application environment (development, staging, production)
REGULENS_ENVIRONMENT=production

# Application version (automatically set by build system)
REGULENS_VERSION=1.0.0

# Instance identifier for multi-instance deployments
REGULENS_INSTANCE_ID=default

# Data center/region identifier
REGULENS_DATACENTER=us-east-1

# =============================================================================
# DATABASE CONFIGURATION
# =============================================================================

# Primary database connection (PostgreSQL recommended for compliance data)
# REQUIRED: Must be explicitly configured - no localhost defaults in production
DB_HOST=your_database_host_here
DB_PORT=5432
DB_NAME=regulens_compliance
DB_USER=regulens_user
DB_PASSWORD=CHANGE_THIS_STRONG_PASSWORD
DB_SSL_MODE=require
DB_CONNECTION_POOL_SIZE=10
DB_CONNECTION_TIMEOUT_MS=30000
DB_MAX_RETRIES=3

# Read replica configuration (optional)
DATABASE_READ_REPLICA_HOST=
DATABASE_READ_REPLICA_PORT=5432

# Audit database (separate from operational data for compliance)
# REQUIRED: Must be explicitly configured - no localhost defaults in production
AUDIT_DB_HOST=your_audit_database_host_here
AUDIT_DB_PORT=5432
AUDIT_DB_NAME=regulens_audit
AUDIT_DB_USER=regulens_audit_user
AUDIT_DB_PASSWORD=CHANGE_THIS_STRONG_PASSWORD

# =============================================================================
# MESSAGE QUEUE CONFIGURATION
# =============================================================================

# Event streaming platform (Kafka/Redpanda recommended)
MESSAGE_QUEUE_TYPE=kafka
# REQUIRED: Must be explicitly configured - no localhost defaults in production
MESSAGE_QUEUE_BOOTSTRAP_SERVERS=your_message_queue_host_here:9092
MESSAGE_QUEUE_SECURITY_PROTOCOL=SASL_SSL
