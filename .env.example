# Regulens AI Compliance System - Environment Configuration
# Copy this file to .env and fill in your actual values
# This configuration supports both local development and cloud/on-prem deployment

# =============================================================================
# DATABASE CONFIGURATION (PostgreSQL with pgvector)
# =============================================================================

# Database connection details - configurable for cloud/on-prem deployment
DB_HOST=localhost
DB_PORT=5432
DB_NAME=regulens_compliance
DB_USER=regulens_user
# SECURITY: Generate a strong password for production
# Generate with: openssl rand -base64 32
DB_PASSWORD=your_secure_database_password_here

# PostgreSQL connection pool settings
DB_MAX_CONNECTIONS=20
DB_CONNECTION_TIMEOUT=30

# =============================================================================
# CACHE CONFIGURATION (Redis)
# =============================================================================

# Redis connection details - configurable for cloud/on-prem deployment
REDIS_HOST=localhost
REDIS_PORT=6379
# SECURITY: Generate a strong password for production
# Generate with: openssl rand -base64 32
REDIS_PASSWORD=your_secure_redis_password_here

# Redis connection pool settings
REDIS_MAX_CONNECTIONS=50
REDIS_CONNECTION_TIMEOUT=10

# =============================================================================
# AUTHENTICATION & SECURITY
# =============================================================================

# JWT Configuration - CRITICAL for security
# Generate with: openssl rand -hex 32 (must be at least 32 characters)
JWT_SECRET=your_32_character_minimum_jwt_secret_here
JWT_EXPIRATION_HOURS=1600
SESSION_EXPIRY_HOURS=24

# Data encryption key for sensitive data
# Generate with: openssl rand -hex 32
DATA_ENCRYPTION_KEY=your_32_character_data_encryption_key_here

# =============================================================================
# EXTERNAL API INTEGRATIONS
# =============================================================================

# OpenAI API Configuration - Required for AI features
OPENAI_API_KEY=sk-your-openai-api-key-here
OPENAI_API_BASE_URL=https://api.openai.com/v1
OPENAI_MODEL=gpt-4-turbo-preview
OPENAI_MAX_TOKENS=4000
OPENAI_TEMPERATURE=0.1

# Anthropic Claude API (alternative/fallback)
ANTHROPIC_API_KEY=sk-ant-your-anthropic-key-here
ANTHROPIC_MODEL=claude-3-opus-20240229

# =============================================================================
# REGULATORY MONITORING
# =============================================================================

# Regulatory Monitor Service URL - internal service communication
REGULATORY_MONITOR_URL=http://localhost:8081

# External regulatory data sources
SEC_API_KEY=your_sec_api_key_here
FINRA_API_KEY=your_finra_api_key_here
ESMA_API_KEY=your_esma_api_key_here

# =============================================================================
# WEB SERVER CONFIGURATION
# =============================================================================

# Server binding - use 0.0.0.0 for all interfaces in production
WEB_SERVER_HOST=0.0.0.0
WEB_SERVER_PORT=8080

# CORS configuration - restrict to your domain in production
CORS_ALLOWED_ORIGIN=http://localhost:3000

# SSL/TLS Configuration (for production)
SSL_CERT_PATH=/path/to/ssl/certificate.crt
SSL_KEY_PATH=/path/to/ssl/private.key
SSL_ENABLED=false

# =============================================================================
# RISK ASSESSMENT CONFIGURATION
# =============================================================================

# Geographic risk tiers (0.0 = lowest risk, 1.0 = highest risk)
JURISDICTION_RISK_TIER_EXTREME=1.0
JURISDICTION_RISK_TIER_HIGH=0.8
JURISDICTION_RISK_TIER_MODERATE=0.5
