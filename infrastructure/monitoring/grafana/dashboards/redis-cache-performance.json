{
  "dashboard": {
    "id": null,
    "title": "Regulens - Redis Cache Performance",
    "tags": ["regulens", "redis", "cache", "performance"],
    "timezone": "browser",
    "panels": [
      {
        "id": 1,
        "title": "Redis Connection Health",
        "type": "stat",
        "targets": [
          {
            "expr": "up{job=\"redis\"}",
            "legendFormat": "Redis Status"
          }
        ]
      },
      {
        "id": 2,
        "title": "Redis Operations Rate",
        "type": "graph",
        "targets": [
          {
            "expr": "rate(regulens_redis_operations_total[5m])",
            "legendFormat": "Total Operations/sec"
          },
          {
            "expr": "rate(regulens_redis_operations_successful_total[5m])",
            "legendFormat": "Successful Operations/sec"
          }
        ]
      },
      {
        "id": 3,
        "title": "Redis Cache Hit Rate",
        "type": "graph",
        "targets": [
          {
            "expr": "regulens_redis_cache_hit_rate",
            "legendFormat": "Cache Hit Rate %"
          }
        ]
      },
      {
        "id": 4,
        "title": "Redis Cache Hits vs Misses",
        "type": "graph",
        "targets": [
          {
            "expr": "rate(regulens_redis_cache_hits_total[5m])",
            "legendFormat": "Cache Hits/sec"
          },
          {
            "expr": "rate(regulens_redis_cache_misses_total[5m])",
            "legendFormat": "Cache Misses/sec"
          }
        ]
      },
      {
        "id": 5,
        "title": "Redis Response Time",
        "type": "graph",
        "targets": [
          {
            "expr": "histogram_quantile(0.95, rate(regulens_redis_request_duration_seconds_bucket[5m]))",
            "legendFormat": "P95 Response Time"
          },
          {
            "expr": "histogram_quantile(0.50, rate(regulens_redis_request_duration_seconds_bucket[5m]))",
            "legendFormat": "P50 Response Time"
          },
          {
            "expr": "regulens_redis_avg_response_time_ms / 1000",
            "legendFormat": "Average Response Time"
          }
        ]
      },
      {
        "id": 6,
        "title": "Redis Connection Pool",
        "type": "graph",
        "targets": [
          {
            "expr": "regulens_redis_pool_connections_total",
            "legendFormat": "Total Connections"
          },
          {
            "expr": "regulens_redis_pool_connections_active",
            "legendFormat": "Active Connections"
          },
          {
            "expr": "regulens_redis_pool_connections_available",
            "legendFormat": "Available Connections"
          }
        ]
      },
      {
        "id": 7,
        "title": "Redis Cache Operations by Type",
        "type": "graph",
        "targets": [
          {
            "expr": "rate(regulens_redis_cache_operations_total[5m])",
            "legendFormat": "{{cache_type}} Operations/sec"
          }
        ]
      },
      {
        "id": 8,
        "title": "Redis Memory Usage",
        "type": "graph",
        "targets": [
          {
            "expr": "regulens_redis_memory_usage_bytes / 1024 / 1024",
            "legendFormat": "Memory Usage (MB)"
          }
        ]
      },
      {
        "id": 9,
        "title": "Redis Cache Size",
        "type": "graph",
        "targets": [
          {
            "expr": "regulens_redis_cache_entries_current",
            "legendFormat": "Current Cache Entries"
          }
        ]
      },
      {
        "id": 10,
        "title": "Redis Evictions",
        "type": "graph",
        "targets": [
          {
            "expr": "rate(regulens_redis_cache_evictions_total[5m])",
            "legendFormat": "Evictions/sec"
          }
        ]
      },
      {
        "id": 11,
        "title": "Redis Error Rate",
        "type": "graph",
        "targets": [
          {
            "expr": "(rate(regulens_redis_operations_total[5m]) - rate(regulens_redis_operations_successful_total[5m])) / rate(regulens_redis_operations_total[5m]) * 100",
            "legendFormat": "Error Rate %"
          }
        ]
      }
    ],
    "time": {
      "from": "now-1h",
      "to": "now"
    },
    "timepicker": {},
    "templating": {
      "list": []
    },
    "annotations": {
      "list": []
    },
    "refresh": "30s",
    "schemaVersion": 27,
    "version": 0,
    "links": []
  }
}
